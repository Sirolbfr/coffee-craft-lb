{
  "name": "cafe",
  "version": "1.0.0",
  "description": "Site vitrine pour un coffee shop fictif",
  "main": "./public/main.js",
  "scripts": {
    "build": "postcss src/css/styles.css -o public/styles.css",
    "watch": "postcss src/css/styles.css -o public/styles.css --watch",
    "start": "live-server public",
    "dev": "tailwindcss -i ./src/input.css -o ./src/output.css --watch"
  },
  "author": "Ali, Luigi, Loris",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "live-server": "^1.2.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "postcss": "^8.5.3"
  }
}
